# Client app (required)
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-supabase-anon-key

# Country config (optional; defaults to nyc)
# Supported: nyc | ch
VITE_COUNTRY=nyc

# Google Maps Places API (optional; enables address autocomplete)
VITE_GOOGLE_MAPS_API_KEY=

# App-level passcode gate (optional; if unset, passcode gate is effectively disabled)
VITE_APP_PASSCODE=

# Observability (optional)
# Example endpoint: https://logs.your-company.com/intake
VITE_OBSERVABILITY_ENDPOINT=
VITE_OBSERVABILITY_ENV=development

# Script/ops env (needed for seed/cleanup/backfill scripts)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
